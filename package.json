  "scripts": {
    "copy-debug": "npx ncp ./src/utils/server_url.debug.ts ./src/utils/server_url.ts",
    "copy-release": "npx ncp ./src/utils/server_url.release.ts ./src/utils/server_url.ts",
    "start": "npm run start:debug",
    "start:debug": "npm run copy-debug && react-scripts start",
    "start:release": "npm run copy-release && react-scripts start",
    "build": "copy-debug && react-scripts build",
    "test": "copy-debug && react-scripts test --transformIgnorePatterns 'node_modules/(?!(@ionic/react|@ionic/react-router|@ionic/core|@stencil/core|ionicons)/)'",
    "eject": "copy-debug && react-scripts eject",
    "electron:debug": "npm run copy-debug && ionic build && npx cap copy @capacitor-community/electron && npx cap open @capacitor-community/electron",
    "electron:debug:make": "npm run copy-debug && ionic build && npx cap copy @capacitor-community/electron && cd ./electron && npm run electron:make",
    "electron:release": "npm run copy-release && ionic build && npx cap copy @capacitor-community/electron && npx cap open @capacitor-community/electron",
    "electron:release:make": "npm run copy-release && ionic build && npx cap copy @capacitor-community/electron && cd ./electron && npm run electron:make"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "4.5.0",
    "@ionic/v1-toolkit": "3.2.20",
    "ncp": "^2.0.0"
  },
  "description": "An Ionic project"
}
